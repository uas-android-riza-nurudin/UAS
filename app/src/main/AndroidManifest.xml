<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.wp.kosku.app">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@drawable/ic_launcher_foreground"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Info" />
        <activity android:name=".About" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ActivityCRUDMain"></activity>
        <activity android:name=".ActivityCRUDCreate"></activity>
        <activity android:name=".ActivityCRUDView"></activity>
        <activity android:name=".ActivityCRUDSingleView"></activity>
        <activity android:name=".Dashboard"></activity>
    </application>

</manifest>